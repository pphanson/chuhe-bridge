<div class='chuhe-alarm-container'>
    <div class='chuhe-alarm-title'>
        <div class="chuhe-alarm-name">报警列表</div>
        
        <div class="chuhe-alarmCondition-select">
            <div class="form">
                <span class="chuhe-select-alarm-type">
                    <span class="chuhe-type-name">报警类型：</span>
                    <a class="chuhe-titleName dropdown-button btn" data-activates='alarmType'>全部<i class="mdi-navigation-arrow-drop-down right"></i></a>
                    <ul id="alarmType" class='dropdown-content'>
                        <li value="0" selected="selected"><span class="-text">全部</span></li>
                        <li value="1"><span class="-text">数值报警</span></li>
                        <li value="2"><span class="-text">传感器报警</span></li>
                    </ul>
                </span>
                <span class="chuhe-select-alarm-level">
                    <span class="chuhe-type-name">报警级别：</span>
                    <a class="chuhe-titleName dropdown-button btn" data-activates='alarmLevel'>全部<i class="mdi-navigation-arrow-drop-down right"></i></a>
                    <ul id="alarmLevel" class='dropdown-content'>
                        <li value="0" selected="selected"><span href="#!" class="-text">全部</span></li>
                        <li value="1"><span href="#!" class="-text">一级告警</span></li>
                        <li value="2"><span href="#!" class="-text">二级告警</span></li>
                        <li value="3"><span href="#!" class="-text">三级告警</span></li>
                    </ul>
                </span>

                <span class="chehe-select-time">
                    <span class="chuhe-titleName">时间范围:</span>
                    <input type="text" id="beginTime">
                    <span class="chuhe-time">至:</span>
                    <input type="text" id="endTime">
                </span>

                <span class="chuhe-select-content">
                    <input type="text" placeholder="    搜索关键字">
                    <button class="waves-effect waves-light" id="searchBtn"><i class="mdi-action-search"></i></button>

                </span>
            </div>
        </div>

    </div>
    <div class="chuhe-alarm-table">
        <table>
            <thead>
                <tr>
                    <th>报警时间</th>
                    <th>报警类型</th>
                    <th>报警级别</th>
                    <th>报警对象</th>
                    <th>对象说明</th>
                    <th>报警值</th>
                </tr>
            </thead>
            <tbody id="tbody"></tbody>
        </table>


    </div>

    <ul class="pagination right" id="pagination2"></ul>

